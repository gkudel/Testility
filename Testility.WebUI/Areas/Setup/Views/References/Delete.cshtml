@model Testility.WebUI.Areas.Setup.Models.ReferencesViewModel

@{
    ViewBag.Title = "Delete[" + Model.Name + "]";
}
@using (Html.BeginForm("Delete", "References", FormMethod.Post, new { id = "Delete", role = "form" }))
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(model=>model.Id)
    <div class="panel panel-default">
        <div class="panel-heading">
            <div><h2><strong><span class="glyphicon glyphicon-trash"></span> Are you sure you want to delete this Reference?</strong></h2></div>
        </div>
        <div class="panel-body">
            <dl class="dl-horizontal">
                <dt>
                    @Html.DisplayNameFor(model => model.Name)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Name)
                </dd>
            </dl>
        </div>
        <div class="panel-footer">
            <div class="text-center">
            
           
                 

                    <div class="table">
                        <div class="td-left">
                            <a href="@Url.Action("List")">
                                <span class="glyphicon glyphicon-step-backward"></span> Back to Projects Explorer
                            </a>
                        </div>
                        <div class="td-right">
                            <button type="submit" value="Save" class="btn btn-danger btn-lg" form="Delete">
                                <span class="glyphicon glyphicon-floppy-disk"></span> Save
                            </button>
                        </div>
                    </div>
            
            </div>
        </div>
    </div>
}
